{
  "name": "@tx-agent-kit/worker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bash -lc 'set -a; [[ -f ../../.env ]] && source ../../.env; set +a; export NODE_OPTIONS=\"--inspect=${WORKER_INSPECT_PORT:-9229}\"; exec tsx watch src/index.ts'",
    "build": "tsc -b",
    "type-check": "tsc -b --pretty false",
    "lint": "eslint src --max-warnings=0",
    "test": "vitest run -c vitest.config.ts",
    "test:integration": "vitest run -c vitest.integration.config.ts",
    "test:integration:temporal": "vitest run -c vitest.temporal.integration.config.ts"
  },
  "dependencies": {
    "@sentry/node": "^10.40.0",
    "@temporalio/client": "^1.13.1",
    "@temporalio/worker": "^1.13.1",
    "@temporalio/workflow": "^1.13.1",
    "@tx-agent-kit/logging": "workspace:*",
    "@tx-agent-kit/observability": "workspace:*"
  },
  "devDependencies": {
    "@tx-agent-kit/vitest-config": "workspace:*"
  }
}
