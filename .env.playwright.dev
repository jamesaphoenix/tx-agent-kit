# Playwright MCP auth bootstrap (1Password references only; no plaintext secrets)
# Run with: op run --env-file=.env.playwright.dev -- pnpm mcp:playwright:auth
PLAYWRIGHT_AUTH_EMAIL=op://octospark-services/dev/PLAYWRIGHT_AUTH_EMAIL
PLAYWRIGHT_AUTH_PASSWORD=op://octospark-services/dev/PLAYWRIGHT_AUTH_PASSWORD
PLAYWRIGHT_AUTH_NAME=op://octospark-services/dev/PLAYWRIGHT_AUTH_NAME
PLAYWRIGHT_AUTH_SITE_URL=http://localhost:3000
PLAYWRIGHT_AUTH_API_BASE_URL=http://localhost:4000
PLAYWRIGHT_AUTH_ALLOW_PROD=false
PLAYWRIGHT_MCP_STORAGE_STATE=.artifacts/playwright-mcp/storage-state.json
