{
  "name": "tx-agent-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "bash -lc 'set -a; [[ -f ./.env ]] && source ./.env; set +a; pnpm infra:ensure; pnpm temporal:dev:up; exec turbo run dev --parallel'",
    "dev:web": "turbo run dev --filter=@tx-agent-kit/web",
    "dev:api": "turbo run dev --filter=@tx-agent-kit/api",
    "dev:worker": "turbo run dev --filter=@tx-agent-kit/worker",
    "dev:mobile": "turbo run dev --filter=@tx-agent-kit/mobile",
    "dev:docs": "turbo run dev --filter=@tx-agent-kit/docs",
    "mobile:generate:api": "pnpm openapi:generate && pnpm --filter @tx-agent-kit/mobile generate:api",
    "build": "turbo run build",
    "build:quiet": "./scripts/build-quiet.sh",
    "env:configure": "./scripts/configure-local-env.sh",
    "openapi:generate": "pnpm --filter @tx-agent-kit/api openapi:generate",
    "api:client:generate": "pnpm openapi:generate && pnpm --filter @tx-agent-kit/web generate:api && pnpm --filter @tx-agent-kit/mobile generate:api",
    "docs:api:generate": "pnpm openapi:generate && tsx apps/docs/scripts/generate-api-docs.ts",
    "type-check": "turbo run type-check",
    "type-check:quiet": "./scripts/type-check-quiet.sh",
    "lint": "turbo run lint && pnpm lint:invariants && pnpm lint:ci-env && pnpm lint:shell",
    "lint:quiet": "./scripts/lint-quiet.sh",
    "lint:invariants": "node ./scripts/lint/enforce-domain-invariants.mjs && node ./scripts/lint/enforce-web-client-contracts.mjs && node ./scripts/lint/enforce-route-kind-contracts.mjs && node ./scripts/lint/enforce-source-type-safety.mjs && node ./scripts/lint/enforce-compose-runtime-contracts.mjs && node ./scripts/lint/enforce-tsconfig-alignment.mjs",
    "lint:ci-env": "node ./scripts/validate-ci-env.mjs",
    "lint:shell": "./scripts/check-shell-invariants.sh",
    "test": "turbo run test",
    "test:quiet": "./scripts/test-quiet.sh",
    "test:integration": "./scripts/test/run-integration.sh",
    "test:integration:quiet": "./scripts/test-integration-quiet.sh",
    "test:boilerplate": "./scripts/test/run-boilerplate.sh",
    "test:boilerplate:quiet": "./scripts/test-boilerplate-quiet.sh",
    "test:db:pgtap": "./scripts/test/run-pgtap.sh",
    "test:infra:observability": "./scripts/test/check-observability.sh",
    "test:run-silent": "./scripts/test-run-silent.sh",
    "verify:temporal-types": "tsx ./scripts/verify-temporal-types.ts",
    "test:temporal-types": "vitest run -c scripts/vitest.config.ts",
    "test:temporal:integration": "./scripts/test/run-temporal-integration.sh",
    "test:temporal:cloud:integration": "./scripts/test/run-temporal-cloud-integration.sh",
    "test:dev:hot-reload": "./scripts/test/dev-hot-reload.sh",
    "mcp:prometheus": "./scripts/mcp/prometheus.sh",
    "mcp:jaeger": "./scripts/mcp/jaeger.sh",
    "mcp:context7": "./scripts/mcp/context7.sh",
    "mcp:supabase": "./scripts/mcp/supabase.sh",
    "mcp:playwright": "./scripts/mcp/playwright.sh",
    "playwright:auth:bootstrap": "node ./scripts/playwright/bootstrap-auth-storage-state.mjs",
    "mcp:playwright:auth": "./scripts/mcp/playwright-auth.sh",
    "mcp:codex-config": "./scripts/mcp/codex-config.sh",
    "tx": "node ./scripts/tx-cli.mjs",
    "infra:ensure": "./scripts/start-dev-services.sh",
    "infra:up": "./scripts/start-dev-services.sh",
    "infra:down": "docker compose -p tx-agent-kit down -v",
    "temporal:dev:up": "./scripts/temporal/start-dev.sh",
    "temporal:dev:down": "./scripts/temporal/stop-dev.sh",
    "temporal:dev:status": "./scripts/temporal/status.sh",
    "worktree:ports": "./scripts/worktree/derive-ports.sh",
    "db:migrate": "pnpm --filter @tx-agent-kit/db db:migrate",
    "db:trigger:new": "node ./scripts/tx-cli.mjs db:trigger:new",
    "db:test:reset": "./scripts/test/reset-test-db.sh",
    "db:studio": "pnpm --filter @tx-agent-kit/db db:studio",
    "scaffold:crud": "node ./scripts/tx-cli.mjs scaffold:crud",
    "deploy:build-images": "./scripts/deploy/build-images.sh",
    "deploy:migrate:staging": "./scripts/deploy/migrate.sh staging",
    "deploy:migrate:prod": "./scripts/deploy/migrate.sh prod",
    "deploy:staging": "./scripts/deploy/deploy-compose.sh staging",
    "deploy:prod": "./scripts/deploy/deploy-compose.sh prod",
    "deploy:smoke": "./scripts/deploy/smoke-api.sh",
    "deploy:gcp:toy:bootstrap": "./scripts/deploy/gcp/bootstrap-toy-project.sh",
    "deploy:gcp:toy:teardown": "./scripts/deploy/gcp/teardown-toy-project.sh",
    "test:gcp:e2e": "./scripts/deploy/gcp/test-e2e.sh",
    "test:gcp:e2e:teardown": "./scripts/deploy/gcp/teardown-toy-project.sh",
    "sync:agent-docs": "./scripts/sync-agent-docs.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.18.6",
    "eslint": "^9.39.2",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "tsx": "^4.21.0",
    "turbo": "^2.7.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "patchedDependencies": {
      "@openrouter/sdk@0.9.11": "patches/@openrouter__sdk@0.9.11.patch"
    }
  }
}
